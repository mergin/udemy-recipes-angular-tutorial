{
    "name": "udemy-recipes-angular-tutorial",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "prebuild": "npm run lint",
        "build": "ng build --aot",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "prebuild:browser": "npm run lint",
        "build:browser": "ng build --prod --aot --subresourceIntegrity",
        "prebuild:server": "npm run lint",
        "build:server": "ng run udemy-recipes-angular-tutorial:server",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "build:everything": "npm run build:browser && npm run build:server && npm run webpack:server",
        "server": "node dist/server.js",
        "check": "npm run lint && npm run build"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/http": "^7.2.10",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/platform-server": "^7.2.10",
        "@angular/router": "~7.2.0",
        "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
        "bootstrap": "^3.4.1",
        "core-js": "^2.5.4",
        "express": "^4.16.4",
        "firebase": "^5.9.0",
        "rxjs": "~6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.6",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.4.0",
        "ts-loader": "^5.3.3",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2",
        "webpack-cli": "^3.3.0"
    }
